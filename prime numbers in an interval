n = int(input())
def is_prime(n):
    if n < 2:
        return False
    for i in range(2,int(n**0.5)+1):
        if n % i == 0:
            return False
    return True
def count_primes(n):
    count = 0
    for num in range(1,n+1):
        if is_prime(num):
            count += 1
    return count
print(count_primes(n))       
