s,t = map(str,input().split())
def isIsomorphicStr(s,t):
    if len(s) != len(t):
        return False
    s_t = {}
    t_s = {}
    for char_s,char_t in zip(s,t):
        if char_s in s_t:
            if s_t[char_s]!= char_t:
                return False
        else:
            s_t[char_s] = char_t
        if char_t in t_s:
            if t_s[char_t]!= char_s:
                return False
        else:
            t_s[char_t] = char_s
    return True
print(isIsomorphicStr(s,t))
